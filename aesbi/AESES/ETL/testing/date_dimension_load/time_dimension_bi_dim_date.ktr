        <class_source><![CDATA[import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Date;
import java.util.TimeZone;
import java.text.DateFormat;

 String nameField;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
	//
	Object[] r = getRow();

    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}

	// Let's look up parameters only once for performance reason.
	//
	if (first) {
		
		nameField = getParameter("NAME_FIELD");
	    first=false;
	}

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
	//
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
try{
		String fff=get(Fields.In, "timestamp_gmt").getString(r);
		Date dt = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss").parse(fff);
		 TimeZone fromTimezone =TimeZone.getTimeZone("GMT");//get Timezone object
	      TimeZone toTimezone=TimeZone.getTimeZone("America/New_York");
	             
	      long fromOffset = fromTimezone.getOffset(dt.getTime());//get offset
	      System.out.println(fromOffset);
	      long toOffset = toTimezone.getOffset(dt.getTime());
	     System.out.println(toOffset);
	      //calculate offset difference and calculate the actual time
	      long convertedTime = dt.getTime() - (fromOffset - toOffset);
	      Date d2 = new Date(convertedTime);
	      String  timestamp = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss").format(d2);
	
	get(Fields.Out, nameField).setValue(outputRow, timestamp);
    
    // putRow will send the row on to the default output hop.
	//
    putRow(data.outputRowMeta, outputRow);
}catch(Exception e){

}

	return true;
}]]></class_source>
