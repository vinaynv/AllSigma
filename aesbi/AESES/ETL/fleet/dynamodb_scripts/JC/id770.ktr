 UNIX_TIMESTAMP(TIMESTAMP)*1000 AS startdate,
 UNIX_TIMESTAMP(DATE_ADD(TIMESTAMP, INTERVAL 1 DAY))*1000 AS enddate
 FROM bi_dim_date WHERE id = (SELECT MAX(bi_dim_date_id) FROM bi_fact_fleet_kpis WHERE bi_dim_fleet_id = 300 AND disposition  IS NOT NULL);


