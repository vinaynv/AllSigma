        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.io.*;

import aes.aws.*;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();

	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first){
        first = false;
    }

				


    r = createOutputRow(r, data.outputRowMeta.size());
	Long startdate = get(Fields.In, "startdate").getInteger(r);
	Long enddate = get(Fields.In, "enddate").getInteger(r);
	//String text = AWSTest.results(6, 1347667200000L,1347926400000L );
	String text = AWSTest.results(770, startdate,enddate );
    
    // Set a value in a new output field
    get(Fields.Out, "output").setValue(r, text);

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);


	return true;
}

]]></class_source>
