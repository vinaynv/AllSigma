    <connection>aeses</connection>
    <sql>SELECT b.timestamp,
avg(a.`Accepted_Availability_mwh_available`) as Accepted_Availability_mwh_available,
avg(a.`gross_revenue_RMCP`) as gross_revenue_RMCP,
avg(a.`energy_losses_mwh`) as energy_losses_mwh,
avg(a.`potential_revenue`) as potential_revenue,
avg(a.`energy_cost`) as energy_cost,
avg(a.`BidAmount`) as BidAmount
 FROM 
(SELECT bi_dim_date_id,
Accepted_Availability_mwh_available,
gross_revenue_RMCP,
energy_losses_mwh,
potential_revenue,
energy_cost,
BidAmount
FROM
`bi_fact_fleet_kpis` WHERE bi_dim_fleet_id=500 AND jira_id =0 AND `Accepted_Availability_mwh_available` IS NOT NULL AND `gross_revenue_RMCP` IS NOT NULL AND 
bi_dim_date_id BETWEEN (SELECT id FROM bi_dim_date WHERE TIMESTAMP=?) AND (SELECT id FROM bi_dim_date WHERE TIMESTAMP=?))a
INNER JOIN 
(SELECT id,TIMESTAMP FROM bi_dim_date WHERE TIMESTAMP BETWEEN ? AND ?)b
ON a.bi_dim_date_id=b.id	
GROUP BY MONTH(b.timestamp),DATE(b.timestamp),HOUR(b.timestamp)</sql>
    <limit>0</limit>
    <lookup>LM From &amp; To Dates</lookup>
    <execute_each_row>N</execute_each_row>
